void setup() {
  size (400,450);
  background (255);
}

void draw (){
  // Circulo rojo
  fill (255,0,0);
  ellipse (200,100,40,40);
  // Circulo naranja
  fill(255,128,0); 
  ellipse (260,130,40,40);
  // Circulo amarillo
  fill (255,255,0);
  ellipse (300,180,40,40);
  // Circulo lima
  fill (191, 255, 0);
  ellipse (325,240,40,40);
  // Circulo verde
  fill (0,255,0);
  ellipse (305,300,40,40);
  // Circulo verde cian
  fill (0, 255, 127);
  ellipse (265,350,40,40);
  // Circulo cian
  fill (0, 255, 255);
  ellipse (200,380,40,40);
  // Circulo azur
  fill (0,161,236);
  ellipse (140,350,40,40);
  // Circulo azul
  fill (0,0,255);
  ellipse (95,300,40,40);
  // Circulo violeta
  fill (127,0,255);
  ellipse (75,240,40,40);
  // Circulo magenta
  fill(255, 0, 255);
  ellipse (100,180,40,40);
  // Circulo fucsia
  fill (255, 0, 196);
  ellipse (140,130,40,40);
  
}
